{
  "name": "mcsrc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:javadoc": "vite --mode javadoc",
    "build": "tsc -b && vite build",
    "build:javadoc": "tsc -b && vite build --mode javadoc",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src tests --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src tests --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\" \"tests/**/*.{js,ts,jsx,tsx}\" && eslint src tests --ext .js,.ts,.jsx,.tsx --fix",
    "format:prettier": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\" \"tests/**/*.{js,ts,jsx,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,json,css,md}\" \"tests/**/*.{js,ts,jsx,tsx}\"",
    "check": "eslint src tests --ext .js,.ts,.jsx,.tsx"
  },
  "dependencies": {
    "@katana-project/zip": "^0.7.1",
    "@monaco-editor/react": "^4.7.0",
    "@run-slicer/vf": "^0.3.2-1.11.2",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.10.0",
    "antd": "^5.28.0",
    "comlink": "^4.4.2",
    "dagre": "^0.8.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rxjs": "^7.8.2",
    "typescript-eslint": "^8.52.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.39.2",
    "globals": "^16.4.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-comlink": "^5.3.0",
    "vitest": "^4.0.15"
  }
}
